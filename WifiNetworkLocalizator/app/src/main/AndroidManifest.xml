<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="skyandroid.wifinetworklocalizator">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.StartUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.AnonymousUser.ChoosingRoomActivity"></activity>
        <activity android:name=".View.AnonymousUser.LocalizationInRoomActivity"></activity>
        <activity android:name=".View.AdminUser.ChooseAdminModeActivity"></activity>
        <activity android:name=".View.AdminUser.ChooseDeterminantMacIdsActivity"/>
        <activity android:name=".View.AdminUser.MeasurmentsActivity"/>
    </application>

</manifest>